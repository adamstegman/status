  <div class="container">
    <main>
      <h2>Current Status</h2>
      <p>Updated <time datetime="2025-05-25">May 25, 2025</time></p>
      <div class="cards">
        <section class="card" itemscope itemtype="https://schema.org/MusicAlbum">
          <div class="card-img">
            <img
             src="assets/supraliminal.jpg"
             alt="Cover image for Supraliminal by Kaela"
             width="240"
             itemprop="image" />
          </div>
          <div class="card-text">
            <h4 class="date-tag"><time datetime="2025-05-16">2025-05-16</time></h4>
            <h3
              ><a
               href="https://kaelasmusic.com"
               itemprop="url"
                ><i itemprop="name">Supraliminal</i></a
              ></h3
            >
            <div>
              by
              <span itemprop="byArtist" itemscope itemtype="https://schema.org/MusicGroup"
                ><span itemprop="name">Kaela</span></span
              >
            </div>
            <h4 class="tag">🎧 Listening</h4>
          </div>
        </section>
